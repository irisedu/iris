{
	"name": "iris-backend",
	"description": "Backend for Iris website",
	"version": "0.0.5",
	"type": "module",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "tsc",
		"start": "NODE_ENV=production node out/index.js",
		"dev": "NODE_ENV=development nodemon -r dotenv/config src/index.ts | pino-pretty",
		"db": "kysely",
		"db:codegen": "kysely-codegen --out-file ./src/db/db.d.ts --dialect postgres"
	},
	"devDependencies": {
		"@types/archiver": "^7.0.0",
		"@types/cookie-parser": "^1.4.10",
		"@types/express": "^5.0.6",
		"@types/express-session": "^1.18.2",
		"@types/formidable": "^3.4.6",
		"@types/mime-types": "^3.0.1",
		"@types/pg": "^8.16.0",
		"@types/unzipper": "^0.10.10",
		"dotenv": "^17.2.3",
		"kysely-codegen": "^0.19.0",
		"kysely-ctl": "^0.20.0",
		"nodemon": "^3.1.11",
		"patchouli": "workspace:^",
		"pino-pretty": "^13.1.3",
		"ts-node": "^10.9.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.980.0",
		"@aws-sdk/lib-storage": "^3.980.0",
		"@irisedu/schemas": "workspace:^",
		"archiver": "^7.0.1",
		"connect-redis": "^8.0.3",
		"cookie-parser": "^1.4.7",
		"csrf-csrf": "^4.0.3",
		"express": "^5.2.1",
		"express-session": "^1.19.0",
		"formidable": "^3.5.4",
		"googleapis": "^171.0.0",
		"ioredis": "^5.9.2",
		"kysely": "^0.28.11",
		"mime-types": "^3.0.2",
		"ollama": "^0.6.3",
		"pg": "^8.18.0",
		"pino": "^10.3.0",
		"unzipper": "^0.12.3",
		"zod": "^4.3.6"
	}
}
