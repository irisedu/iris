{
	"name": "iris-backend",
	"description": "Backend for Iris website",
	"version": "0.0.5",
	"type": "module",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "tsc",
		"start": "NODE_ENV=production node out/index.js",
		"dev": "NODE_ENV=development nodemon -r dotenv/config src/index.ts | pino-pretty",
		"db": "kysely",
		"db:codegen": "kysely-codegen --out-file ./src/db/db.d.ts --dialect postgres"
	},
	"devDependencies": {
		"@types/archiver": "^6.0.3",
		"@types/cookie-parser": "^1.4.9",
		"@types/express": "^5.0.3",
		"@types/express-session": "^1.18.2",
		"@types/formidable": "^3.4.5",
		"@types/mime-types": "^3.0.1",
		"@types/pg": "^8.15.5",
		"@types/unzipper": "^0.10.10",
		"dotenv": "^17.2.1",
		"kysely-codegen": "^0.18.5",
		"kysely-ctl": "^0.17.0",
		"nodemon": "^3.1.10",
		"patchouli": "workspace:^",
		"pino-pretty": "^13.1.1",
		"ts-node": "^10.9.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.879.0",
		"@aws-sdk/lib-storage": "^3.879.0",
		"@irisedu/schemas": "workspace:^",
		"archiver": "^7.0.1",
		"connect-redis": "^8.0.3",
		"cookie-parser": "^1.4.7",
		"csrf-csrf": "^4.0.3",
		"express": "^5.1.0",
		"express-session": "^1.18.2",
		"formidable": "^3.5.4",
		"googleapis": "^157.0.0",
		"ioredis": "^5.7.0",
		"kysely": "^0.28.5",
		"mime-types": "^3.0.1",
		"ollama": "^0.5.17",
		"pg": "^8.16.3",
		"pino": "^9.9.0",
		"unzipper": "^0.12.3",
		"zod": "^4.1.0"
	}
}
